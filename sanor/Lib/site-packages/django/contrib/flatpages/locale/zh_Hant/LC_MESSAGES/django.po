#: .\sanor\Lib\site-packages\django\contrib\flatpages\admin.py:12
msgid "Advanced options"
msgstr "進階選項"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\apps.py:7
msgid "Flat Pages"
msgstr "簡平頁面"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\forms.py:9
#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:9
msgid "URL"
msgstr "URL"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\forms.py:12
msgid ""
"Example: '/about/contact/'. Make sure to have leading and trailing slashes."
msgstr "例如： '/about/contact/'。開頭與結束的斜線。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\forms.py:15
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"此欄位只能有: 英文字母、數字、點(.)、底線(_)、刪節號(-)、斜線(/)、波浪號(~)。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\forms.py:29
#, fuzzy
#| msgid ""
#| "Example: '/about/contact/'. Make sure to have leading and trailing "
#| "slashes."
msgid "Example: '/about/contact'. Make sure to have a leading slash."
msgstr "例如： '/about/contact/'。開頭與結束的斜線。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\forms.py:42
msgid "URL is missing a leading slash."
msgstr "URL 缺少前導的斜線。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\forms.py:47
msgid "URL is missing a trailing slash."
msgstr "URL 缺少一個斜線。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\forms.py:64
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "網站 %(site)s 已經存在一個浮動頁面，其位址為 %(url)s。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:10
msgid "title"
msgstr "標題"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:11
msgid "content"
msgstr "內容"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:12
msgid "enable comments"
msgstr "啟用評論"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:14
msgid "template name"
msgstr "模板名稱"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:18
msgid ""
"Example: 'flatpages/contact_page.html'. If this isn't provided, the system "
"will use 'flatpages/default.html'."
msgstr ""
"例如： 'flatpages/contact_page'。如果沒有提供它，系統會使用 'flatpages/"
"default.html'。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:23
msgid "registration required"
msgstr "請先註冊"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr "如果此項被選取，則只有登入的使用者可以檢視頁面。"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:27
msgid "sites"
msgstr "網站"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:31
msgid "flat page"
msgstr "簡平頁面"

#: .\sanor\Lib\site-packages\django\contrib\flatpages\models.py:32
msgid "flat pages"
msgstr "簡平頁面"
