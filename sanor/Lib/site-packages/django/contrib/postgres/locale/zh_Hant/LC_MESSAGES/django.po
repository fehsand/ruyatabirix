#: .\sanor\Lib\site-packages\django\contrib\postgres\apps.py:42
msgid "PostgreSQL extensions"
msgstr "PostgreSQL 擴充"

#: .\sanor\Lib\site-packages\django\contrib\postgres\fields\array.py:22
#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\array.py:16
#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\array.py:170
#, fuzzy, python-format
#| msgid "Item %(nth)s in the array did not validate: "
msgid "Item %(nth)s in the array did not validate:"
msgstr "陣列中第 %(nth)s 個物件驗證失敗："

#: .\sanor\Lib\site-packages\django\contrib\postgres\fields\array.py:23
msgid "Nested arrays must have the same length."
msgstr "各嵌套陣列長度必須相同。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\fields\hstore.py:16
msgid "Map of strings to strings/nulls"
msgstr "字串與字串/空值的對應"

#: .\sanor\Lib\site-packages\django\contrib\postgres\fields\hstore.py:18
#, python-format
msgid "The value of \"%(key)s\" is not a string or null."
msgstr "「%(key)s」並非字串或空值。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:32
msgid "A JSON object"
msgstr "一個 JSON 物件"

#: .\sanor\Lib\site-packages\django\contrib\postgres\fields\jsonb.py:34
msgid "Value must be valid JSON."
msgstr "必須為合法 JSON 值。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\hstore.py:16
msgid "Could not load JSON data."
msgstr "無法載入 JSON 資料。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "必須輸入 JSON dictionary。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\jsonb.py:19
#, python-format
msgid "'%(value)s' value must be valid JSON."
msgstr "'%(value)s' 必須為合法 JSON 值。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\ranges.py:19
msgid "Enter two valid values."
msgstr "請輸入兩個有效的值"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\ranges.py:20
msgid "The start of the range must not exceed the end of the range."
msgstr "範圍的起始不可超過範圍的結束。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\ranges.py:67
msgid "Enter two whole numbers."
msgstr "請輸入兩個整數"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\ranges.py:73
msgid "Enter two numbers."
msgstr "請輸入兩個數字"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\ranges.py:90
msgid "Enter two valid date/times."
msgstr "請輸入兩個有效的日期/時間"

#: .\sanor\Lib\site-packages\django\contrib\postgres\forms\ranges.py:96
msgid "Enter two valid dates."
msgstr "請輸入兩個有效的日期"

#: .\sanor\Lib\site-packages\django\contrib\postgres\validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] "串列包含 %(show_value)d 個物件，但不應包含多於 %(limit_value)d 個。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] "串列包含 %(show_value)d 個物件，但應至少包含 %(limit_value)d 個。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "缺少鍵值：%(keys)s"

#: .\sanor\Lib\site-packages\django\contrib\postgres\validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "包含不明鍵值：%(keys)s"

#: .\sanor\Lib\site-packages\django\contrib\postgres\validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "請確認此範圍是否完全小於或等於 %(limit_value)s。"

#: .\sanor\Lib\site-packages\django\contrib\postgres\validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr "請確認此範圍是否完全大於或等於 %(limit_value)s。"
